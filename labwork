#include<stdio.h>
int main(void)
{
int number;
printf("Enter a number between 0 and 100:");
scanf("%d", &number);
if(number>=90)
{
printf("A");
}
else if(number>=75)
{
printf("B");
}
else if(number>=55)
{
printf("C");
}
else if(number>=45)
{
printf("D");
}
else if(number<45)
{
printf("F");
}
return (0);
}
